<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta name="description" content="" />
    <meta name="author" content="TemplateMo" />
    <link
      href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900"
      rel="stylesheet"
    />
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">


    <title>Applications</title>

    <link href="/css/bootstrap.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="/stylesheets/fontawesome.css" />
    <link rel="stylesheet" href="/stylesheets/templatemo-edu-meeting.css" />
    <link rel="stylesheet" href="/stylesheets/owl.css" />
    <link rel="stylesheet" href="/stylesheets/lightbox.css" />
    <link rel="stylesheet" href="/stylesheets/applications.css" />
    <link rel="stylesheet" href="/stylesheets/index.css" />
  </head>

  <body>
    <section class="upcoming-meetings" id="meetings">
      <div class="" id="container">
        <!-- ***** Header Area Start ***** -->
        <header class="header-area header-sticky">
          <div class="container">
            <div class="row">
              <div class="col-12">
                <nav class="main-nav">
                  <!-- ***** Logo Start ***** -->
                  <a href="index" class="logo">
                    <img src="/images/logo_white.png" alt="JMI" />
                    PLACEMENT CELL
                  </a>
                  <!-- ***** Logo End ***** -->
                  <!-- ***** Menu Start ***** -->
                  <ul class="nav">
                    <li><a href="index" class="active">Logout</a></li>
                  </ul>

                  <a class="menu-trigger">
                    <span>Menu</span>
                  </a>
                  <!-- ***** Menu End ***** -->
                </nav>
              </div>
            </div>
          </div>
        </header>
        <!-- ***** Header Area End ***** -->

        <form class="form-signin">
          <table>
            <thead>
              <tr>
                <th>STUDENT ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Course</th>
                <th>SPA</th>
                <th>EMAIL</th>
              </tr>
            </thead>

            <tbody>
              <% for(let i=0; i<ApplicantsData.length; i++) {%>
              <tr>
                <td> <%=ApplicantsData[i].studentID %></td>
                <td> <%=ApplicantsData[i].firstname %></td>
                <td> <%=ApplicantsData[i].lastName %></td>
                <td> <%=ApplicantsData[i].course %></td>
                <td> <%=ApplicantsData[i].spi %></td>
                <td> <%=ApplicantsData[i].email_address %></td>
              </tr>
              <% } %>
            </tbody>
          </table>
        </form>
      </div>
    </section>
  </body>
</html>
